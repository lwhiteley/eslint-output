{
  "extends": [
    "airbnb-base"
  ],
  "plugins": [],
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-underscore-dangle" : 0,
    "max-len": [1, 180, 4],
    "arrow-body-style": [0],
    "no-param-reassign": 0,
    "global-require": 0,
    "no-console": 0,
    "import/no-unresolved": [0] // Until import plugin supports webpack 2 resolveModules
  }
}